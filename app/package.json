{
    "name": "app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
      "clean:dist": "rm -rf dist/",
      "clean:all": "rm -rf dist/ && rm -rf node_modules/",
      "clean:modules": "rm -rf node_modules/",
      "dev": "vite",
      "tsc": "vue-tsc",
      "build:client": "vite build",
      "build:cart": "LIB_NAME=cart vite build",
      "build:server": "LIB_NAME=server vite build --ssr src/server.ts",
      "build:all": "npm run build:cart && npm run build:server",
      "build:dependencies": "npm run build:hypernova-vue && npm run build:uikit",
      "build:hypernova-vue": "cd /techflexers-monorepo/libraries/javascript/npm/hypernova-vue && npm install && npm run build",
      "build:uikit": "cd /techflexers-monorepo/libraries/javascript/npm/uikit && npm install && npm run build ",
      "link:dependencies": "cd node_modules/@techflexers && ln -s /techflexers-monorepo/libraries/javascript/npm/hypernova-vue hypernova-vue",
      "link:uikit": "cd node_modules/@techflexers && ln -s /techflexers-monorepo/libraries/javascript/npm/uikit uikit",
      "dev:server": "npm install && vue-tsc && npm run build:server && npm run server",
      "dev:cart": "LIB_NAME=cart vite build --watch",
      "server": "node dist/server.js",
      "test": "vitest run",
      "test:watch": "vitest"
    },
    "dependencies": {
      "@babel/runtime": "^7.21.5",
      "@fortawesome/fontawesome-svg-core": "^6.4.0",
      "@fortawesome/free-brands-svg-icons": "^6.4.0",
      "@fortawesome/free-regular-svg-icons": "^6.4.0",
      "@fortawesome/free-solid-svg-icons": "^6.4.0",
      "@fortawesome/vue-fontawesome": "^3.0.3",
      "@headlessui/tailwindcss": "^0.2.0",
      "@headlessui/vue": "^1.7.15",
      "@privateLib/uikit": "file:../library/uikit/",
      "@vueuse/components": "^10.3.0",
      "@vueuse/core": "^10.3.0",
      "hypernova": "^2.5.0",
      "reusify": "^1.0.4",
      "vue": "^3.3.4"
    },
    "devDependencies": {
      "@tailwindcss/container-queries": "^0.1.1",
      "@tailwindcss/forms": "^0.5.3",
      "@testing-library/jest-dom": "^6.1.3",
      "@testing-library/user-event": "^14.5.1",
      "@testing-library/vue": "^7.0.0",
      "@types/express": "^4.17.17",
      "@vitejs/plugin-vue": "^4.2.3",
      "@vitest/coverage-v8": "^0.34.1",
      "@vitest/ui": "^0.34.6",
      "@vue/test-utils": "^2.4.1",
      "autoprefixer": "^10.4.14",
      "chroma-js": "^2.4.2",
      "gsap": "^3.12.2",
      "jsdom": "^22.1.0",
      "msw": "^1.2.3",
      "prettier-plugin-tailwindcss": "^0.5.4",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "tailwindcss": "^3.3.3",
      "tailwindcss-themer": "^3.1.0",
      "typescript": "^5.0.4",
      "vite": "^4.4.9",
      "vitest": "^0.34.1",
      "vue-tsc": "^1.8.8"
    }
}